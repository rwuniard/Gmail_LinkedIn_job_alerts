# Logging Configuration
# LOG_LEVEL: DEBUG, INFO, WARNING, ERROR, CRITICAL (default: INFO)
LOG_LEVEL=INFO

# LOG_FORMAT: json, text, or dual (default: dual)
# - json: JSON format to stdout (for CloudWatch/observability)
# - text: Human-readable text to stderr (for console)
# - dual: Both JSON (stdout) and text (stderr)
LOG_FORMAT=dual

# ENVIRONMENT: development or production (default: development)
# - development: Detailed logging format
# - production: Compact logging format
ENVIRONMENT=development

# ActiveMQ Configuration
HOST = 'localhost'
PORT = 61616
USERNAME = '********'
PASSWORD = '********'
DESTINATION = '/queue/linkedin_jobs'